@using JicoDotNet.Inventory.BusinessLayer.DTO.Class;
@using JicoDotNet.Inventory.Core.Models
@using JicoDotNet.Inventory.UI.Models;
 

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Inventory Solution | @ViewBag.Title</title>
    <meta name="description" content="Welcome to Inventory Solution. Full Inventory Managment and Payment">
    <meta name="keywords" content="Item, Blob, Inventory Solution, Inspiron Edge">
    <meta name="author" content="Inspiron Edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="theme-color" content="#fff" @*content="#ffc107"*@ />
    @*<link rel="manifest" href="~/Content/pwa/manifest.json" />*@

    <!-- Fav Icon -->
    <link rel="icon" href="~/favicon.ico">

    <!-- Font Awesome (all.min.css- v5.12.1) -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css">
    <!-- Ionicons v2.0.1 -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Select2 -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.10/css/select2.min.css">
    <!-- Toastr -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.3/toastr.css" />
    <!-- Theme style v3.0.2 -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/admin-lte/3.0.2/css/adminlte.min.css">
    <!-- Data Tables -->
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.22/css/dataTables.bootstrap4.min.css" />
    <!-- Data Tables Responsive -->
    <link rel="stylesheet" href="https://cdn.datatables.net/responsive/2.2.7/css/responsive.bootstrap.min.css" />
    <!-- Data Tables Button -->
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/1.7.0/css/buttons.dataTables.min.css" />

    <!--datepicker4 // https://gijgo.com/datepicker/configuration-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/gijgo/1.9.13/combined/css/gijgo.min.css" />
    <!-- Bootstrap4-duallistbox v4.0.1 -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap4-duallistbox/4.0.1/bootstrap-duallistbox.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700">
    <!-- jQuery UI: Styles -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css" />
    <!-- Inventory Solution Custom Style -->
    <link rel="stylesheet" href="~/Content/css/AdminLteCustom.css?v=1.5" />

    <!-- jQuery v3.5.1 -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <!-- jQueryui v1.12.1 -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>

    @RenderSection("HeadCss", false)
</head>
<body class="sidebar-mini layout-fixed text-sm layout-navbar-fixed">
    <div class="wrapper">
        <!-- Navbar -->
        @Html.Partial("_PartialNavbarMain")
        <!-- /.navbar -->
        <!-- .sidebar -->
        <aside class="main-sidebar elevation-4 sidebar-light-success">
            <!-- Brand Logo -->
            @Html.Partial("_PartialBrandLogo")
            <!-- Sidebar -->
            <div class="sidebar" data-sidebar>
                <!-- Sidebar user panel (optional) -->
                @Html.Partial("_PartialSidebarUser", (CompanyBasic)ViewBag.Company)
                <!-- Sidebar Menu -->
                @if (Html.Area() == "Report")
                {
                    @Html.Action("AsideReport", "Partial", new { Area = string.Empty })
                }
                else
                {
                    @Html.Action("AsideMain", "Partial", new { Area = string.Empty })
                }
            </div>
        </aside>
        <!-- /.sidebar -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        @Html.Partial("_PartialFooter")
        <!-- Control Sidebar -->
        @Html.Partial("_PartialSidebar", new SessionCredentialCompanyModels { _sessionCredential = ViewBag.SessionPerson, _company = ViewBag.Company })
    </div>

    @Html.Partial("_PartialScript")

    <!--Datatable button-->
    <!--https://datatables.net/extensions/buttons/examples/initialisation/export.html-->
    <!--https://datatables.net/extensions/buttons/examples/html5/simple.html-->
    @RenderSection("BodyScript", false)
    @*<script src="~/Content/pwa/main.js"></script>*@
</body>
</html>
